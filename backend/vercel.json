{
    "version": 2,
    "name": "node-js",
    "builds": [
        { "src": "index.ts", "use": "@vercel/node" }
    ],
    "routes": [
        {
      "src": "/(.*)",
      "dest": "./index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
  }
    ]
}